{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "restaurant_project/wsgi.py",
        "use": "@vercel/python-wsgi"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "restaurant_project/wsgi.py"
      }
    ]
  }
  